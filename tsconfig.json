{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "./src/types/svg.d.ts"
  ]
}